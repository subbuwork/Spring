<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config/>
    <bean id="profile" class="com.spring.annotations.Profile"></bean>
    
    <bean id="student" class="com.spring.annotations.Student">
    <property name="sid" value="1000"></property>
    <property name="sName" value="Subbarao"></property>
    </bean>
    
    <bean id="student2" class="com.spring.annotations.Student">
    <property name="sid" value="1002"></property>
    <property name="sName" value="neelima"></property>
    </bean>
    <bean id="student3" class="com.spring.annotations.Student">
    <property name="sid" value="1003"></property>
    <property name="sName" value="Aadhya"></property>
    </bean>
    
    <bean id="textEditor" class="com.spring.annotations.TextEditor"></bean>
    <bean id="checker" class="com.spring.annotations.SpellChecker"></bean>
    
    </beans>